<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.webapi.models.MentionedAsWitch" table="wdb_mentionedaswitch" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="mentionref">
            <column name="mentionref" sql-type="char(50)" length="50"/>
        </id>
        <property name="mentionsystemid">
            <column name="mentionsystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="mentionid">
            <column name="mentionid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="mentiondate">
            <column name="mentiondate" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="mentiondateAsDate">
            <column name="mentiondate_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="mentiontype">
            <column name="mentiontype" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="trialofaccusedref">
            <column name="trialofaccusedref" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="trialoffirstname">
            <column name="trialoffirstname" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="trialoflastname">
            <column name="trialoflastname" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="mentionedintrialofref">
            <column name="mentionedintrialofref" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="dateofmentionedtrial">
            <column name="dateofmentionedtrial" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="dateofmentionedtrialAsDate">
            <column name="dateofmentionedtrial_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="fateofmentionedwitch">
            <column name="fateofmentionedwitch" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="notes">
            <column name="notes" sql-type="text" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.webapi.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>