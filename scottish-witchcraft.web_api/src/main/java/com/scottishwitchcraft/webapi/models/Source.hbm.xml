<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.webapi.models.Source" table="wdb_source" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="sourceref">
            <column name="sourceref" sql-type="char(50)" length="50"/>
        </id>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <set name="wdbReferencesBySourceref" inverse="true">
            <key>
                <column name="sourceref" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.scottishwitchcraft.webapi.models.Reference"/>
        </set>
    </class>
</hibernate-mapping>