<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.webapi.models.AccusedFamily" table="wdb_accused_family" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="accusedFamilyref">
            <column name="accused_familyref" sql-type="char(20)" length="20"/>
        </id>
        <property name="accusedFamilysystemid">
            <column name="accused_familysystemid" sql-type="char(3)" length="3" not-null="true"/>
        </property>
        <property name="accusedFamilyid">
            <column name="accused_familyid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="surname">
            <column name="surname" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="firstname">
            <column name="firstname" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="alias">
            <column name="alias" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="patronymic">
            <column name="patronymic" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="destitle">
            <column name="destitle" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="estYearOfBirth">
            <column name="est_year_of_birth" sql-type="integer" not-null="true"/>
        </property>
        <property name="age">
            <column name="age" sql-type="bigint" not-null="true"/>
        </property>
        <property name="ageEstcareer">
            <column name="age_estcareer" sql-type="boolean" not-null="true"/>
        </property>
        <property name="ageEstchild">
            <column name="age_estchild" sql-type="boolean" not-null="true"/>
        </property>
        <property name="occupation">
            <column name="occupation" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="relationship">
            <column name="relationship" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbAccusedByAccusedref" class="com.scottishwitchcraft.webapi.models.WdbAccused">
            <column name="accusedref" not-null="true"/>
        </many-to-one>
        <set name="wdbComplaintsByAccusedFamilyref" inverse="true">
            <key>
                <column name="accused_familyref" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.scottishwitchcraft.webapi.models.Complaint"/>
        </set>
    </class>
</hibernate-mapping>