<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.webapi.models.MovesToHLA" table="wdb_movestohla" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="moveref">
            <column name="moveref" sql-type="char(50)" length="50"/>
        </id>
        <property name="movesystemid">
            <column name="movesystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="moveid">
            <column name="moveid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="moveto">
            <column name="moveto" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="moveexists">
            <column name="moveexists" sql-type="boolean" not-null="true"/>
        </property>
        <property name="movedate">
            <column name="movedate" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="movedateAsDate">
            <column name="movedate_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.webapi.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>