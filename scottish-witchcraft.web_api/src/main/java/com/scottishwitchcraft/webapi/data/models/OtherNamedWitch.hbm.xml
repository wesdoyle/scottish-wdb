<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.webapi.models.OtherNamedWitch" table="wdb_othernamedwitch" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="namedwitchref">
            <column name="namedwitchref" sql-type="char(50)" length="50"/>
        </id>
        <property name="namedwitchsystemid">
            <column name="namedwitchsystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="namedwitchid">
            <column name="namedwitchid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="mentiontype">
            <column name="mentiontype" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="namedwitchaccusedref">
            <column name="namedwitchaccusedref" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="namedwitchfirstname">
            <column name="namedwitchfirstname" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="namedwitchlastname">
            <column name="namedwitchlastname" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="namedwitchtrialofref">
            <column name="namedwitchtrialofref" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="dateofnamedwitchtrial">
            <column name="dateofnamedwitchtrial" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="dateofnamedwitchtrialAsDate">
            <column name="dateofnamedwitchtrial_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="notes">
            <column name="notes" sql-type="text" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.webapi.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>