<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.webapi.models.Ordeal" table="wdb_ordeal" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="ordealref">
            <column name="ordealref" sql-type="char(50)" length="50"/>
        </id>
        <property name="ordealsystemid">
            <column name="ordealsystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="ordealid">
            <column name="ordealid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="ordealexists">
            <column name="ordealexists" sql-type="boolean" not-null="true"/>
        </property>
        <property name="ordealdate">
            <column name="ordealdate" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="ordealdateAsDate">
            <column name="ordealdate_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="ordealtype">
            <column name="ordealtype" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.webapi.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>