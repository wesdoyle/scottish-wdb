<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.dao.models.Torture" table="wdb_torture" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="tortureref">
            <column name="tortureref" sql-type="char(50)" length="50"/>
        </id>
        <property name="torturesystemid">
            <column name="torturesystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="tortureid">
            <column name="tortureid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="tortureexists">
            <column name="tortureexists" sql-type="boolean" not-null="true"/>
        </property>
        <property name="torturedate">
            <column name="torturedate" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="torturedateAsDate">
            <column name="torturedate_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="torturetype">
            <column name="torturetype" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.dao.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>