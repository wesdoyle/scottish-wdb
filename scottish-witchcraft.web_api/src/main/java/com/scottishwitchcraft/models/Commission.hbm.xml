<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.dao.models.Commission" table="wdb_commission" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="commissionref">
            <column name="commissionref" sql-type="char(50)" length="50"/>
        </id>
        <property name="commissionsystemid">
            <column name="commissionsystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="commissionid">
            <column name="commissionid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="commbody">
            <column name="commbody" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="commtype">
            <column name="commtype" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="commexists">
            <column name="commexists" sql-type="boolean" not-null="true"/>
        </property>
        <property name="commdate">
            <column name="commdate" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="commdateAsDate">
            <column name="commdate_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="seal">
            <column name="seal" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="notes">
            <column name="notes" sql-type="text" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.dao.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>