<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.dao.models.Denunciation" table="wdb_denunciation" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="denuncref">
            <column name="denuncref" sql-type="char(50)" length="50"/>
        </id>
        <property name="denuncsystemid">
            <column name="denuncsystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="denuncid">
            <column name="denuncid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="denuncExists">
            <column name="denunc_exists" sql-type="boolean" not-null="true"/>
        </property>
        <property name="denuncdate">
            <column name="denuncdate" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="denuncdateAsDate">
            <column name="denuncdate_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="denuncText">
            <column name="denunc_text" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.dao.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>