<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.dao.models.Imprisonment" table="wdb_imprisonment" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="imprisonref">
            <column name="imprisonref" sql-type="char(50)" length="50"/>
        </id>
        <property name="imprisonsystemid">
            <column name="imprisonsystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="imprisonid">
            <column name="imprisonid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="centraltrialimprison">
            <column name="centraltrialimprison" sql-type="boolean" not-null="true"/>
        </property>
        <property name="imprisondate">
            <column name="imprisondate" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="imprisondateAsDate">
            <column name="imprisondate_as_date" sql-type="date" not-null="true"/>
        </property>
        <property name="prison">
            <column name="prison" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="location">
            <column name="location" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="moving">
            <column name="moving" sql-type="boolean" not-null="true"/>
        </property>
        <property name="fateInPrison">
            <column name="fate_in_prison" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbTrialByTrialref" class="com.scottishwitchcraft.dao.models.Trial">
            <column name="trialref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>