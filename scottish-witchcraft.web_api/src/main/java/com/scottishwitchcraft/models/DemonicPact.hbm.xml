<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scottishwitchcraft.dao.models.DemonicPact" table="wdb_demonicpact" schema="public"
           catalog="ScottishWitchcraft.Dev">
        <id name="demonicref">
            <column name="demonicref" sql-type="char(50)" length="50"/>
        </id>
        <property name="demonicsystemid">
            <column name="demonicsystemid" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="demonicid">
            <column name="demonicid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="demonicType">
            <column name="demonic_type" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="demonicText">
            <column name="demonic_text" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="date" not-null="true"/>
        </property>
        <property name="lastupdatedby">
            <column name="lastupdatedby" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="lastupdatedon">
            <column name="lastupdatedon" sql-type="date" not-null="true"/>
        </property>
        <many-to-one name="wdbCaseByCaseref" class="com.scottishwitchcraft.models.Case">
            <column name="caseref" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>